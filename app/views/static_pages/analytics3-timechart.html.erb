      [ 'Session 3', 'Session',       new Date(0,0,0,12,0,0),  new Date(0,0,0,13,30,0) ],
      [ 'Break 3', 'Break',    new Date(0,0,0,14,0,0),  new Date(0,0,0,15,30,0) ],
      [ 'Session 4', 'Session',       new Date(0,0,0,12,0,0),  new Date(0,0,0,13,30,0) ],
