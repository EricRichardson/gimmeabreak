    var options = {
      timeline: { colorByRowLabel: true },
      title: 'Time in Minutes Between Suggested Break and Actual Break Taken',
      curveType: 'function',
      legend: { position: 'bottom' },
      'width':1000,
      'height':500,
      'backgroundColor':'aliceblue',
         animation:{
           startup:true,
           duration:2000
         },
         titleTextStyle:{
